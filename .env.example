# MIDNIGHT FIGHTER - Environment Configuration
# Copy this file to .env and fill in your MongoDB Atlas credentials

# MongoDB Atlas Connection String
# Format: mongodb+srv://username:password@cluster-name.mongodb.net/?retryWrites=true&w=majority
MONGO_URI=mongodb+srv://username:password@your-cluster.mongodb.net/midnight_fighter?retryWrites=true&w=majority

# Optional: API Port (default: 3000)
# PORT=3000

# Optional: Node Environment
# NODE_ENV=development
